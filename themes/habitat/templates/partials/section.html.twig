{% for item in root.children %}
        <h4>{{ item.menu }}</h4>
        <ul class="list-unstyled">
{% for child in item.children %}
{% if child.visible %}
            <li>&nbsp;<i class="fa fa-chevron-right color-gray-lighter"></i>&nbsp;<a href="{{ child.url }}">{{ child.title }}</a></li>
{% endif %}
{% endfor %}
        </ul>
{% endfor %}
